{
  "name": "nodejs-restful-jsonapi-seed",
  "version": "1.9.0",
  "description": "Everything you need to start building a scalable web application.",
  "main": "src/app.js",
  "scripts": {
    "build": "node_modules/.bin/babel src -s -D -d dist",
    "deploy": "node_modules/.bin/pm2 start dist/app.js -i max --no-daemon",
    "docker": "docker build -t app . --build-arg NODE_ENV=${NODE_ENV:-development} --no-cache && docker run -d -p3000:3000 app",
    "genapi": "swagger-jsdoc -d src/swagger.js -o swagger.json src/routes/**.js",
    "gendoc": "node_modules/.bin/jsdoc -d doc -r src",
    "lint": "node_modules/.bin/eslint --ignore-path .gitignore src test",
    "start": "node dist/app.js",
    "test": "node_modules/.bin/mocha 'test/**/*.test.js' --require @babel/register --require test/mocha.env.js --exit",
    "watch": "node_modules/.bin/nodemon -w src --exec \"npm run build ; npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxy/nodejs-restful-jsonapi-seed.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "es6",
    "restful",
    "jsonapi",
    "docker"
  ],
  "author": "Marc S. Brooks <devel@mbrooks.info> (https://mbrooks.info)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuxy/nodejs-restful-jsonapi-seed/issues"
  },
  "homepage": "https://github.com/nuxy/nodejs-restful-jsonapi-seed#readme",
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "commander": "^5.1.0",
    "config": "^3.3.8",
    "copy-dir": "^1.3.0",
    "cors": "^2.8.5",
    "deep-copy": "^1.4.2",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "jsonapi-query-parser": "^1.3.1",
    "jsonapi-serializer": "^3.6.7",
    "memorystore": "^1.6.7",
    "morgan": "^1.9.1",
    "pm2": "^5.2.2",
    "rotating-file-stream": "^2.1.6",
    "swagger-doc": "^0.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.0",
    "@babel/register": "^7.17.7",
    "@node-minify/clean-css": "^4.0.5",
    "@node-minify/cli": "^6.2.0",
    "@node-minify/uglify-js": "^4.0.5",
    "babel-loader": "^8.2.5",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-transform-html-import-to-string": "^2.0.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "chance": "^1.1.9",
    "eslint": "^8.31.0",
    "glob": "^7.2.3",
    "jsdoc": "^3.6.11",
    "mocha": "^9.2.2",
    "mocha-shared": "^0.2.0",
    "nodemon": "^2.0.20",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0"
  },
  "overrides": {
    "glob-parent": "^6.0.0",
    "got": "^12.0.0",
    "micromatch": "^4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bin": {
    "seed-cli": ".bin/cli.js"
  }
}
